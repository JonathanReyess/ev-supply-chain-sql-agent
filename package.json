{
  "name": "ev-supply-chain-sql-agent",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "copy-assets": "mkdir -p dist/prompts dist/data && cp src/prompts/* dist/prompts/ && cp data/* dist/data/",
    "build": "npm run clean && tsc && npm run copy-assets",
    "start": "node dist/agent.js",
    "dev": "npx ts-node --esm src/agent.ts",
    "api": "npx tsx src/api.ts",
    "api:summary": "npx tsx src/api.ts",
    "api:embeddings": "npx tsx src/api-embeddings.ts",
    "test:conversation": "npx tsx src/test/compare-conversation-methods.ts",
    "frontend": "npx http-server frontend -p 3000 -c-1 --cors",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@google/genai": "^1.26.0",
    "canvas": "^3.2.0",
    "chart.js": "^4.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "duckdb": "^1.1.3",
    "express": "^5.1.0",
    "http-server": "^14.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@duckdb/duckdb-wasm": "^1.29.0",
    "@types/chart.js": "^2.9.41",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/node": "^22.10.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3",
    "vite": "^6.0.3"
  }
}
